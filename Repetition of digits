ANS2:
#include<iostream>
using namespace std;

int main() {
    int n = 5;
    int arr[5];
    bool repetition = false;

    cout << "Enter the 5 elements of the array: " << endl;

    for (int i = 0; i < n; i++)
{
        cin >> arr[i];
    }
   
    for (int i = 0; i < n-1; i++)
{
        for (int j = i + 1; j < n; j++)
{
            if (arr[i] == arr[j])
{
                repetition = true;
                break;
            }
        }
        if (repetition)
{
            break;
        }
    }

    if (repetition)
{
        cout << "True" << endl;
    } else {
        cout << "False" << endl;
    }

    return 0;
}
